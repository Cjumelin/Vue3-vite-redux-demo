{
  "author": "cjumelin@protonmail.com",
  "name": "Vuejs3-Vite-Redux-starter",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "dev": "vite",
    "debug": "vite --debug",
    "build": "vite build",
    "test": "jest --watchAll",
    "story": "cucumber-js -p default"
  },
  "dependencies": {
    "@reduxjs/redux-devtools-extension-fork": "^0.0.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "rxjs": "^6.5.5",
    "typescript": "^3.9.3",
    "vite": "^0.15.3",
    "vue": "^3.0.0-beta.10"
  },
  "devDependencies": {
    "@vue/test-utils": "^2.0.0-alpha.6",
    "vue-jest": "^3.0.5",
    "ts-jest": "^26.0.0",
    "jest": "^26.0.1",
    "@types/jest": "^25.2.3",
    "@vue/compiler-sfc": "^3.0.0-beta.10",
    "cucumber": "^6.0.5",
    "cucumber-pretty": "^6.0.0",
    "expect": "^25.1.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts",
      "vue"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest",
      ".*\\.(vue)$": "vue-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "testRegex": "(.(\\.|/)(test|spec))\\.(jsx?|tsx?)$"
  }
}
